  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "sidebar-style": {
      "title": "gitbook_guide",
      "author": "Mealoore"
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "showGoTop": true
