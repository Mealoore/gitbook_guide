  "plugins": [
    "theme-default",
    "-search",
    "-lunr",
    "-sharing",
    "-livereload",
    "sharing-plus",
    "github",
    "chart",
    "todo",
    "graph",
    "puml",
    "katex",
    "copy-code-button",
    "ace",
    "emphasize",
    "mermaid-gb3",
    "flexible-alerts",
    "expandable-chapters-small",
    "anchor-navigation-ex",
    "theme-comscore",
    "splitter",
    "popup",
    "sidebar-style",
    "edit-link",
    "bibtex",
    "tbfed-pagefooter",
    "terminal"
